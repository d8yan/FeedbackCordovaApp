<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.feedbackcordovaapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Feedback Cordova App</name>
    <description>
        An Apache Cordova application that allow user to show,add, edit and delete business feedback .
    </description>
    <author email="dyan4970@conestogac.on.ca" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="icon.png"></icon>
    <splash src="dy-image.png"></splash>
</widget>
